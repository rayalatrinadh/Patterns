package Patterns;

public class Pattern_10 {
    public static void main(String[] args) {
        pattern10(5);
    }
    static void pattern10(int n){
        for(int i = 1; i <=n; i++){
            //5,4,3,2,1 spaces
            for(int j = 1; j <= n - i + 1; j++){
                System.out.print(" ");
            }
            //star
            for(int k = 1; k <= i; k++){
                System.out.print("* ");
            }
            System.out.println();
        }
    }
}
