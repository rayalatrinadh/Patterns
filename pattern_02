package Patterns;

public class Pattern_02 {
    public static void main(String[] args) {
        pattern_02(5);

    }
    static void pattern_02(int n){
        for(int i = 0; i < n; i++){
            for(int j = 0;j <= i; j++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
