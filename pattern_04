package Patterns;

public class Pattern_04 {
    public static void main(String[] args) {
        pattern_04(5);
    }
    static void pattern_04(int n){
        for(int i = 1; i <= n; i++){
            for(int j = 1;j <= i; j++){
                System.out.print(j+" ");
            }
            System.out.println();
        }
    }
}
