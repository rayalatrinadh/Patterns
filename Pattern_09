package Patterns;

public class Pattern_09 {
    public static void main(String[] args) {
        pattern09(5);
    }
    static void pattern09(int n){
        for(int i = 1; i <=n; i++){
            //spaces
            for(int j = 1; j < i;j++){
                System.out.print(" ");
            }

            for(int k = 1; k <= 2 * n - (2*i-1) ; k++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
