package Patterns;

public class Pattern_03 {
    public static void main(String[] args) {
        pattern_03(5);
    }
    static void pattern_03(int n){
        for(int i = 0; i < n; i++){
            for(int j = 0; j < n - i; j++){
                System.out.print("*");
            }
            System.out.println("");
        }
    }
}
